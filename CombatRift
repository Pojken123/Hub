--[script]--
local library = loadstring(game:HttpGet(('https://pastebin.com/raw/FsJak6AT')))()
local w = library:CreateWindow("Combat Rift")
local b = w:CreateFolder("Auto Farm")
local q = w:CreateFolder("Auto Buy")
local e = w:CreateFolder("Mix")
local u = w:CreateFolder("Credits")
local world = "1"
local boss = 6
local EGGS = {"Spawn 1","Spawn 2","Desert 1","Desert 2","Frozen","Lava 1","Lava 2","Demon","Only Good Demon Pets?","Water 1","Water 2","Clockwork"}
local E = 1
local EA = 1
local BOSSES = {"Ancient Bastet","Frozen Lord","Inferno Gladiator","Supreme Warlord","Drowned Pirate Captain","Mechanical Goliath"}

--[Toggle]--
q:Toggle("Souls",function(bool)
    shared.toggle = bool
    AutoBuySouls = bool
end)

q:Toggle("Eggs",function(bool)
    shared.toggle = bool
    AutoBuyEggs = bool
end)

q:Dropdown("Egg",{EGGS[1],EGGS[2],EGGS[3],EGGS[4],EGGS[5],EGGS[6],EGGS[7],EGGS[8],EGGS[9],EGGS[10],EGGS[11],EGGS[12]},true,function(egg)
    for i = 1, #EGGS do
        if egg == EGGS[i] then
            E = i
        end
    end
end)

q:Dropdown("Egg Amount",{"1","3 GamePass Only"},true,function(Value)
    if Value == "1" then
        EA = 1
    else
        EA = 3
    end
end)

b:Toggle("Auto Tp Mobs",function(bool)
    shared.toggle = bool
    AutoTpMobs = bool
end)

b:Dropdown("World 1",{"1","2","3","4","5","6","7"},true,function(Value) 
    world = Value
end)

b:Toggle("Auto Tp Boss",function(bool)
    shared.toggle = bool
    AutoTpBoss = bool
end)

b:Dropdown("Ancient Bastet",{BOSSES[1],BOSSES[2],BOSSES[3],BOSSES[4],BOSSES[5],BOSSES[6],BOSSES[7],BOSSES[8],BOSSES[9],BOSSES[10],BOSSES[11]},true,function(Value) 
    for i = 1, #BOSSES do
        if Value == BOSSES[i] then
            boss = 3 + i * 3
        end
    end
end)

b:Toggle("Auto Sell",function(bool)
    shared.toggle = bool
    AutoSell = bool
end)

b:Toggle("Auto Tp KOTH",function(bool)
    shared.toggle = bool
    AutoTpKOTH = bool
end)

e:Toggle("Anti Afk",function(bool)
    shared.toggle = bool
    AntiAfk = bool
end)

--[Credits]--
u:Button("maxgat5#8395",function()
    setclipboard("maxgat5#8395")
end)

u:Button("Pojken#9816",function()
    setclipboard("Pojken#9816")
end)
 
u:Button("Discrod Server",function()
    setclipboard("https://discord.gg/K4txdRSVfq")
end)

--[Functions]--
while true do
    wait()
    if AutoTpKOTH == true then
        if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").BuildingMapStuff.Map.Map4.Functional.KOTH.Model.MeshPart.CFrame.Position + Vector3.new(0,25,0))
        end
    end
    
    if AutoBuySouls == true then
        for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
            if v.ClassName == "Folder" then
                for i,v1 in pairs(v:GetChildren()) do
                    if v1.ClassName == "RemoteFunction" then
                        game:GetService("ReplicatedStorage")[v.Name][v1.Name]:InvokeServer("PurchaseAllSouls")
                    end
                end
            end
        end
    end

    if AutoBuyEggs == true then
        if EA == 1 then
            for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
                if v.ClassName == "Folder" then
                    for i,v1 in pairs(v:GetChildren()) do
                        if v1.ClassName == "RemoteFunction" then
                            game:GetService("ReplicatedStorage")[v.Name][v1.Name]:InvokeServer("HatchEgg",E)
                        end
                    end
                end
            end
        end
        if EA == 3 then
            for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
                if v.ClassName == "Folder" then
                    for i,v1 in pairs(v:GetChildren()) do
                        if v1.ClassName == "RemoteFunction" then
                            game:GetService("ReplicatedStorage")[v.Name][v1.Name]:InvokeServer("HatchThreeEggs",E)
                        end
                    end
                end
            end
        end
    end
    
    if AutoSell == true then
        local player = game:GetService("Players").LocalPlayer.Name
        string = game:GetService("Players"):FindFirstChild(player).PlayerGui.GameInterface.Currency.Skulls.Front.Amount.Text
        a,b = string:match("(.+)/(.+)")
        if a == b then
            local playerposition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame.Position + Vector3.new(0,0,0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").BuildingMapStuff.Map.Map1.Functional.Sell.Model.Model.Model.E.CFrame.Position + Vector3.new(0,-17,10))
            wait(.2)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").BuildingMapStuff.Map.Map1.Functional.Sell.Model.Model.Model.E.CFrame.Position + Vector3.new(1,-17,20))
            wait(.2)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").BuildingMapStuff.Map.Map1.Functional.Sell.Model.Model.Model.E.CFrame.Position + Vector3.new(0,-17,10))
            wait(.2)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(playerposition)
        end
    end
    
    if AutoTpMobs == true then
        for i,v in pairs(game:GetService("Workspace").EnemySpawnRegions:GetChildren()) do
            function tp()
                for i,v1 in pairs(v:GetChildren()) do
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v1.HumanoidRootPart.CFrame.Position + Vector3.new(0,0,3))
                    end
                    wait()
                end
            end
            
            if world == "1" then
                if v.Name == "1" then
                    tp()
                end
            end
            
            if world == "2" then
                if v.Name == "2" then
                    tp()
                end
            end
            
            if world == "3" then
                if v.Name == "3" then
                    tp()
                end
            end
        
            if world == "4" then
                if v.Name == "4" then
                    tp()
                end
            end
            
            if world == "5" then
                if v.Name == "5" then
                    tp()
                end
            end

            if world == "6" then
                if v.Name == "6" then
                    tp()
                end
            end

            if world == "7" then
                if v.Name == "7" then
                    tp()
                end
            end
        end
    end
    
    if AutoTpBoss == true then
        for i,v in pairs(game:GetService("Workspace").EnemySpawnRegions:GetChildren()) do
            for i,v1 in pairs(v:GetChildren()) do
                if v1.EnemyValue.Value == boss then 
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v1.HumanoidRootPart.CFrame.Position + Vector3.new(0,0,3))
                    end
                end
            end
        end
    end

    if AntiAfk == true then
        local bb=game:service'VirtualUser'
        game:service'Players'.LocalPlayer.Idled:connect(function()
            bb:CaptureController()
            bb:ClickButton2(Vector2.new())
        end)
    end
end
